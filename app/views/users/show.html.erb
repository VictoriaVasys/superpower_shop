<h1> <%= current_user.first_name %>'s Profile Page </h1>

<div class="powers">
  <% @paid_orders.each do |order| %>
    <% order.powers.each do |power| %>
      <div class="power", id= <%= "power_#{power.id}" %>>
        <div class="power_image">
          <img src="<%= show_correct_image(power) %>" width="150" height="150"/>
        </div>
        <h1><%= link_to "#{power.title}", power_path(power) %></h1>
      </div>
    <% end %>
  <% end %>
</div>
